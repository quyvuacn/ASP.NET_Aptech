@model ShopASP.Features.Models.FileModel

<p>@TempData["Message"]</p>
<form class="form-horizontal" asp-controller="Upload" asp-action="UploadImage" method="POST" enctype="multipart/form-data">
    <input type="file" asp-for="FileData">
    <input type="text" asp-for="FileName" placeholder="File Name">
    <input type="submit" value="Submit"/>
</form>
